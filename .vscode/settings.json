{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "rust-analyzer.check.command": "build",
  "rust-analyzer.showSyntaxTree": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "files.associations": {
    "*.sql": "postgres"
  },
  "[postgres]": {
    "editor.tabSize": 2
  },
  "[ungrammar]": {
    "editor.tabSize": 2
  }
}
